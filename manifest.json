{
    "manifest_version": 3,
    "name": "Web to RSS Converter",
    "version": "1.0",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
